{
  "version": 8,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23
  },
  "configurePresets": [
    {
      "name": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "OPENMP_ENABLED": false,
        "EIGEN_ENABLED": false
      },
      "hidden": true,
      "warnings": {
        "deprecated": true
      }
    }
  ]
}
