{
  "version": 8,
  "configurePresets": [
    {
      "name": "ninja-release",
      "inherits": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "ninja-debug",
      "inherits": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "EIGEN_ENABLED": true
      }
    },
    {
      "name": "ninja-omp",
      "inherits": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "OPENMP_ENABLED": true,
        "EIGEN_ENABLED": true
      }
    },
    {
      "name": "ninja-omp-prof",
      "inherits": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "OPENMP_ENABLED": true,
        "EIGEN_ENABLED": true
      }
    }
  ]
}
